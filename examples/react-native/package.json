{
  "name": "aqua-js-sdk-react-native-example",
  "version": "1.0.0",
  "description": "React Native example for aqua-js-sdk",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace AquaExample.xcworkspace -scheme AquaExample -configuration Release -destination generic/platform=iOS -archivePath AquaExample.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "pod-install": "cd ios && pod install",
    "setup": "npm install && npx react-native setup-ios-permissions && npm run pod-install"
  }, 
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "aqua-js-sdk": "file:../../aqua-js-sdk-3.2.1-47.tgz",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "react-native-fs": "^2.20.0",
    "expo-document-picker": "^11.5.4",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "https-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "browserify-zlib": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "react-native",
    "aqua-protocol",
    "blockchain",
    "data-accounting"
  ],
  "author": "Aqua Protocol Team",
  "license": "MIT"
}
