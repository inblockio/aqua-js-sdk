{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "declaration": false,
    "isolatedModules": true
  },
  "include": ["./index.ts", "./aqua-v2-example.ts"],
  "exclude": ["node_modules", "dist", "src"]
}

